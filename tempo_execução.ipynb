   "source": [
    "import time\n",
    "def loopParaSomarTempo():\n",
    "    for i in range(10):\n",
    "        print(\"testando!\")\n",
    "        \n",
    "        \n",
    "inicio = time.time()\n",
    "loopParaSomarTempo()\n",
    "fim = time.time()\n",
    "\n",
    "print(fim - inicio)"
   ]
